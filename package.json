{
  "name": "spaceMaster",
  "version": "1.0.1",
  "description": "SpaceMaster is a full stack web application using React, Node.js, Express and Webpack.",
  "engines": {
    "node": "12.18.4"
  },
  "scripts": {
    "build-client": "webpack --mode production --env.ENVIRONMENT=production --config ./webpack.react.config.js",
    "build-client-local": "webpack --mode development --env.ENVIRONMENT=development --config ./webpack.react.config.js",
    "build-server": "tsc",
    "start":"",
    "start-bak": "npm run build-server && node public_dist/server/index.js",
    "start-wo-build": "node public_dist/server/index.js",
    "client": "webpack-dev-server --mode development --env.ENVIRONMENT=development --devtool inline-source-map --hot --config ./webpack.react.config.js --host 0.0.0.0",
    "server": "nodemon src/server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "author": "Lenno",
  "license": "MIT",
  "dependencies": {
